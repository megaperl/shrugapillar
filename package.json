{
  "name": "shrugapillar",
  "version": "1.0.0",
  "description": "A library for generating shrugapillars.",
  "author": "@ndrwhr",
  "keywords": [
    "shrug",
    "shrugapillar",
    "meme"
  ],
  "license": "MIT",
  "repository": "memeguild/shrugapillar",
  "bugs": {
    "url": "https://github.com/memeguild/shrugapillar/issues"
  },
  "main": "lib/common.js",
  "scripts": {
    "build": "npm run build:js; npm run build:css",
    "build:css": "postcss --config conf/postcss.config.js",
    "build:js": "webpack --config conf/webpack.js",
    "deploy": "gh-pages -d demo",
    "prepublish": "npm run build",
    "watch": "webpack --watch --config conf/webpack.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "cssnano": "^3.7.4",
    "gh-pages": "^0.11.0",
    "postcss-calc": "^5.3.0",
    "postcss-cli": "^2.5.2",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-inline-svg": "^2.0.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "redux": "^3.5.2"
  }
}
